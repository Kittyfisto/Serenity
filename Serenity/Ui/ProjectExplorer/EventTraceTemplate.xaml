    </PathGeometry>

    <DataTemplate DataType="{x:Type projectExplorer:EventTraceViewModel}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Viewbox Width="24" Height="24"
                     Margin="0,0,6,0">
